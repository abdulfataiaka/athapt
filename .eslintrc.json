{
    "env": {
        "node": true,
        "es6": true
    },
    

    "extends": [
        "eslint:recommended"
    ],

    "rules": {
        "indent": [
            "error",
            "space"
        ],
       
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}
